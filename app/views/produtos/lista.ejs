<html>
    <body>
        <table border="1px">
            <tr>
                <td>id</td>
                <td>titulo</td>
                <td>descricao</td>
                <td>preco</td>
            </tr>
            <tr>
                <!-- TABELA FIXA 
                <td>1</td>
                <td>livro de node js</td>
                -->

                <!-- CÓDIGO DINÂMICO PARA MONATAR A TABELA-->
                <%for(var i=0;i<lista.length;i++) {%> <!-- percorre o Json lista, lá do produtos.js -->
                    <tr>
                        <td><%=lista[i].id%></td> <!-- para imprimir valor utiliza-se o "=" -->
                        <td><%=lista[i].titulo%></td>
                        <td><%=lista[i].descricao%></td>
                        <td><%=lista[i].preco%></td>
                    </tr>
                <%}%>    
            </tr>
        </table>
    </body>
</html>